<h1>Project</h1>

<%= link_to 'Cancel', @project %> |
<%= link_to 'Projects', projects_path %>

<% form_for(@project) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :hidden %><br />
    <%= f.check_box :hidden %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

